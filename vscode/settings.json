{
  "breadcrumbs.enabled": true,
  "editor.renderWhitespace": "all",
  "editor.fontFamily": "'Cascadia Mono', Hack, Menlo, Monaco, 'Courier New', monospace",
  "editor.tabSize": 2,
  "editor.renderControlCharacters": false,
  "editor.snippetSuggestions": "top",
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 60,
  "editor.minimap.renderCharacters": false,
  "editor.rulers": [120],
  "explorer.autoReveal": false,
  "prettier.singleQuote": true,
  "markdown.preview.fontSize": 13,
  "files.exclude": {
    "**/.git": false
  },
  "search.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.svn": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/tmp": true
  },
  "cSpell.userWords": ["Gomeeki", "AdvisorZone"],
  "terminal.integrated.fontFamily": "'Cascadia Mono', Hack, Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
  "workbench.sideBar.location": "right",
  "workbench.colorTheme": "GitHub Dark",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.enablePreview": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.tree.indent": 12,
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.list.openMode": "doubleClick",
  "npm.fetchOnlinePackageInfo": false,
  "gitlens.codeLens.includeSingleLineSymbols": true,
  "gitlens.currentLine.scrollable": false,
  "gitlens.defaultGravatarsStyle": "mp",
  "gitlens.hovers.enabled": false,
  "gitlens.views.search.enabled": false,
  "gitlens.views.compare.enabled": false,
  "gitlens.views.repositories.files.layout": "list",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "javascript.implicitProjectConfig.checkJs": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "window.closeWhenEmpty": false,
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
  "window.zoomLevel": 0,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "java.configuration.checkProjectSettingsExclusions": false,
  "javascript.validate.enable": false,
  "diffEditor.renderSideBySide": true,
  "editor.suggestSelection": "first",
  "flow.useLSP": false,
  "java.semanticHighlighting.enabled": true,
  "docker.volumes.description": [
    "CreatedTime"
  ],
  "workbench.activityBar.visible": true,
  "markdownlint.customRules": [
    "{docsmsft.docs-linting}/markdownlint-custom-rules/rules.js"
  ],
  "markdownlint.config": {
    "MD028": false
  },
  "color-highlight.markRuler": false
}
