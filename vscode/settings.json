{
  "breadcrumbs.enabled": true,
  "editor.renderWhitespace": "all",
  "editor.fontFamily": "'Cascadia Mono', Hack, Menlo, Monaco, 'Courier New', monospace",
  "editor.tabSize": 2,
  "editor.renderControlCharacters": false,
  "editor.snippetSuggestions": "top",
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 60,
  "editor.minimap.renderCharacters": false,
  "editor.rulers": [
    100
  ],
  "explorer.autoReveal": false,
  "prettier.singleQuote": true,
  "markdown.preview.fontSize": 13,
  "files.exclude": {
    "**/.git": false
  },
  "search.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.svn": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/tmp": true
  },
  "cSpell.userWords": [
    "AdvisorZone",
    "Gomeeki",
    "customizer"
  ],
  "terminal.integrated.fontFamily": "'Cascadia Mono', Hack, Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
  "workbench.sideBar.location": "right",
  "workbench.colorTheme": "GitHub Dark",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.enablePreview": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.tree.indent": 12,
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.list.openMode": "doubleClick",
  "npm.fetchOnlinePackageInfo": false,
  "gitlens.codeLens.includeSingleLineSymbols": true,
  "gitlens.currentLine.scrollable": false,
  "gitlens.defaultGravatarsStyle": "mp",
  "gitlens.hovers.enabled": false,
  "gitlens.views.search.enabled": false,
  "gitlens.views.compare.enabled": false,
  "gitlens.views.repositories.files.layout": "list",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "javascript.implicitProjectConfig.checkJs": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "window.closeWhenEmpty": false,
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "java.configuration.checkProjectSettingsExclusions": false,
  "javascript.validate.enable": false,
  "diffEditor.renderSideBySide": true,
  "flow.useLSP": false,
  "java.semanticHighlighting.enabled": true,
  "docker.volumes.description": [
    "CreatedTime"
  ],
  "workbench.activityBar.visible": true,
  "markdownlint.customRules": [
    "{docsmsft.docs-linting}/markdownlint-custom-rules/rules.js"
  ],
  "markdownlint.config": {
    "MD028": false
  },
  "color-highlight.markRuler": false,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "editor.quickSuggestionsDelay": 10,
  "diffEditor.ignoreTrimWhitespace": false,
  "jest.coverageFormatter": "GutterFormatter",
  "workbench.editor.showTabs": true,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "editor.fontLigatures": true,
  "extensions.ignoreRecommendations": true,
  "phpfmt.php_bin": "C:\\PHP\\php.exe",
  "phpfmt.indent_with_space": 2,
  "phpformatter.composer": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.enableFiletypes": [
    "!json"
  ]
}